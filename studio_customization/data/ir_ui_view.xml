  <record id="studio_customization.odoo_studio_default__937bfaca-f2ad-49b0-92e7-08230a8f4180" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='x_name']" position="attributes">
          <attribute name="string">item name</attribute>
        </xpath>
        <xpath expr="//group[@name='studio_group_25e574_left']" position="inside">
          <field name="x_studio_cost" string="cost"/>
          <field name="x_studio_profit_margin" string="profit margin"/>
        </xpath>
